{
    "manifest_version": 2,
    "name": "UHD Quick Links",
    "description": "Quick launch of useful UHD Icons",
    "version": "1.0.0",
    "icons": {
        "128": "icon_128.png"
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": ["activeTab"]



}